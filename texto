PreenchimentoProcesso.mover_arquivos(arg_strCaminhoSalvarArquivo = cls._var_dictConfig["arg_CaminhaSalvarPdf"], arg_strConta=var_strConta, arg_stragencia=var_strAgencia)
        PreenchimentoProcesso.renomear_arquivos(arg_strAgencia = var_strAgencia, 
                                                arg_strConta = var_strConta, 
                                                arg_strCaminhoSalvarArquivo = cls._var_dictConfig["arg_CaminhaSalvarPdf"])



 var_strPathArquivo  = cls._var_dictConfig['caminhoTXTauxiliar']
          var_strcontas = arg_strConta.replace('-', '')
          var_strAgencia = arg_stragencia.replace('-', '')
          with open(var_strPathArquivo, 'r') as arquivo:
                var_strMesAno = arquivo.read().strip()

          if not os.path.exists(var_strPathArquivo) or os.path.getsize(var_strPathArquivo)==0 or var_strMesAno == "": 
            Maestro.write_log("Selecionando 'Mês Anterior'")
            var_dateUltimoDiaMesPassado = date.today().replace(day=1) - timedelta(days=1)
            var_datePrimeiroDiaMesPassado = var_dateUltimoDiaMesPassado.replace(day=1)
            var_strPrimeiroDiaMesPassado = var_datePrimeiroDiaMesPassado.strftime("%d_%m_%Y")
            var_strUltimoDiaMesPassado = var_dateUltimoDiaMesPassado.strftime("%d_%m_%Y")
            var_strDia = str(date.today().day)
            var_strMes = str(date.today().month)
            if  int(var_strDia)<10:
               var_strDia = "0"+str(date.today().day)
            if int(var_strMes)<10:
               var_strMes = "0"+str(date.today().month)
               

            var_strNomeArquivoPdf = f"Extrato_{var_strAgencia}_{var_strcontas}_{var_strDia}-{var_strMes}-{str(date.today().year)}.pdf"
            var_strNomeArquivoXlsx = f"Extrato_Lançamentos_{var_strAgencia}_{var_strcontas}_{var_strDia}-{var_strMes}-{str(date.today().year)}.xlsx"
          else:
            var_strDataConvertida = datetime.strptime(var_strMesAno, "%m.%Y")
            var_strPrimeiroDiaMesPassado = var_strDataConvertida.replace(day=1)
            var_strPrimeiroDiaMesPassado = var_strPrimeiroDiaMesPassado.strftime('%d_%m_%Y')
            var_strUltimoDiaMesPassado = var_strDataConvertida.replace(day= calendar.monthrange(var_strDataConvertida.year, var_strDataConvertida.month)[1])
            var_strUltimoDiaMesPassado = var_strUltimoDiaMesPassado.strftime('%d_%m_%Y')
            
            var_strDia = str(date.today().day)
            var_strMes = str(date.today().month)
            if  int(var_strDia)<10:
               var_strDia = "0"+str(date.today().day)
            if int(var_strMes)<10:
               var_strMes = "0"+str(date.today().month)
               

            var_strNomeArquivoPdf = f"Extrato_{var_strAgencia}_{var_strcontas}_{var_strDia}-{var_strMes}-{str(date.today().year)}.pdf"
            var_strNomeArquivoXlsx = f"Extrato_Lançamentos_{var_strAgencia}_{var_strcontas}_{var_strDia}-{var_strMes}-{str(date.today().year)}.xlsx"
